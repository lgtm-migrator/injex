{
    "compilerOptions": {
        "module": "es2015",
        "declaration": true,
        "alwaysStrict": true,
        "importHelpers": true,
        "target": "ES5",
        "noImplicitAny": false,
        "sourceMap": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node",
        "allowJs": true,
        "preserveConstEnums": true,
        "lib": [
			"es6",
			"es7",
			"dom"
		]
    },
    "compileOnSave": false
}